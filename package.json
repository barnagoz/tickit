{
  "name": "tickit",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "eslint": "^8.26.0",
    "eslint-config-react-app": "^7.0.1",
    "react": "^18.2.0",
    "react-app": "^1.1.2",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23"
  },
  "packageManager": "npm@8.11.0"
}
